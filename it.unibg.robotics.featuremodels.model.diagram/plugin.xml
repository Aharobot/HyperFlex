            name="%navigatorContentName" 
            priority="normal" 
            contentProvider="it.unibg.robotics.featuremodels.model.diagram.navigator.FeatureModelNavigatorContentProvider" 
            labelProvider="it.unibg.robotics.featuremodels.model.diagram.navigator.FeatureModelNavigatorLabelProvider"
            icon="icons/obj16/featureModelDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="featuremodel_diagram"/>
               </and>
               <instanceof value="it.unibg.robotics.featuremodels.model.diagram.navigator.FeatureModelAbstractNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
         	   <instanceof value="it.unibg.robotics.featuremodels.model.diagram.navigator.FeatureModelAbstractNavigatorItem"/>
            </or>
         </possibleChildren>
         <commonSorter 
               id="it.unibg.robotics.featuremodels.diagram.navigatorSorter" 
               class="it.unibg.robotics.featuremodels.model.diagram.navigator.FeatureModelNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="featuremodel_diagram"/>
                  </and>
                  <instanceof value="it.unibg.robotics.featuremodels.model.diagram.navigator.FeatureModelAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
