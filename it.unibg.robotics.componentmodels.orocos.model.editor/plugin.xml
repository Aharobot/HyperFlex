<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated orocosComponentModel -->
      <wizard
            id="it.unibg.robotics.orocoscomponentmodel.presentation.orocoscomponentmodelModelWizardID"
            name="%_UI_orocoscomponentmodelModelWizard_label"
            class="it.unibg.robotics.orocoscomponentmodel.presentation.orocoscomponentmodelModelWizard"
            category="it.robotics.unibg.hyperflex.wizard.category"
            icon="icons/full/obj16/orocoscomponentmodelModelFile.gif">
         <description>%_UI_orocoscomponentmodelModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated orocosComponentModel -->
      <editor
            id="it.unibg.robotics.orocoscomponentmodel.presentation.orocoscomponentmodelEditorID"
            name="%_UI_orocoscomponentmodelEditor_label"
            icon="icons/full/obj16/orocoscomponentmodelModelFile.gif"
            extensions="orocoscomponentmodel"
            class="it.unibg.robotics.orocoscomponentmodel.presentation.orocoscomponentmodelEditor"
            contributorClass="it.unibg.robotics.orocoscomponentmodel.presentation.orocoscomponentmodelActionBarContributor">
      </editor>
   </extension>

</plugin>
