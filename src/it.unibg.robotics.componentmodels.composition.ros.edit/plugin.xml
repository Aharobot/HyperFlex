<plugin>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <!-- @generated rosCompositionModel -->
      <factory
            uri="http://robotics.unibg.it/compositionmodels.ros"
            class="org.hyperflex.roscompositionmodel.provider.roscompositionmodelItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

   <extension point="org.eclipse.emf.edit.childCreationExtenders">
      <!-- @generated rosCompositionModel -->
      <extender
            uri="http://robotics.unibg.it/compositionmodels"
            class="org.hyperflex.roscompositionmodel.provider.roscompositionmodelItemProviderAdapterFactory$compositionmodelChildCreationExtender"/>
   </extension>

</plugin>
