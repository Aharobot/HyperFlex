<plugin>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http://www.hyperflex.org/resolutionmodels.ros"
            class="org.hyperflex.resolutionmodels.rosresolutionmodels.provider.rosresolutionmodelsItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

   <extension point="org.eclipse.emf.edit.childCreationExtenders">
      <extender
            uri="http://www.hyperflex.org/resolutionmodels"
            class="org.hyperflex.resolutionmodels.rosresolutionmodels.provider.rosresolutionmodelsItemProviderAdapterFactory$resolutionmodelsChildCreationExtender"/>
   </extension>

</plugin>
