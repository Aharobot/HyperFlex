<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated rosCompositionModel -->
      <wizard
            id="it.unibg.robotics.roscompositionmodel.presentation.roscompositionmodelModelWizardID"
            name="%_UI_roscompositionmodelModelWizard_label"
            class="it.unibg.robotics.roscompositionmodel.presentation.roscompositionmodelModelWizard"
            category="it.robotics.unibg.hyperflex.wizard.category"
            icon="icons/full/obj16/roscompositionmodelModelFile.gif">
         <description>%_UI_roscompositionmodelModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated rosCompositionModel -->
      <editor
            id="it.unibg.robotics.roscompositionmodel.presentation.roscompositionmodelEditorID"
            name="%_UI_roscompositionmodelEditor_label"
            icon="icons/full/obj16/roscompositionmodelModelFile.gif"
            extensions="roscompositionmodel"
            class="it.unibg.robotics.roscompositionmodel.presentation.roscompositionmodelEditor"
            contributorClass="it.unibg.robotics.roscompositionmodel.presentation.roscompositionmodelActionBarContributor">
      </editor>
   </extension>
</plugin>
