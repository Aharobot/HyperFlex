<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <wizard
            id="it.unibg.robotics.resolutionmodel.ros.presentation.rosModelWizardID"
            name="%_UI_rosModelWizard_label"
            class="it.unibg.robotics.resolutionmodels.rosresolutionmodels.presentation.rosresolutionmodelsModelWizard"
            category="it.robotics.unibg.featuremodels.wizard.category"
            icon="icons/full/obj16/rosModelFile.gif">
         <description>%_UI_rosModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="it.unibg.robotics.resolutionmodel.ros.presentation.rosEditorID"
            name="%_UI_rosEditor_label"
            icon="icons/full/obj16/rosModelFile.gif"
            extensions="rosresolutionmodel"
            class="it.unibg.robotics.resolutionmodels.rosresolutionmodels.presentation.rosresolutionmodelsEditor"
            contributorClass="it.unibg.robotics.resolutionmodels.rosresolutionmodels.presentation.rosresolutionmodelsActionBarContributor">
      </editor>
   </extension>

</plugin>
